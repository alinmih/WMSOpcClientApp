{
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.File" ],
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss.fff}[{Level:u3}]<{ThreadId,2}>[{SourceContext,33}]-> {Message}{NewLine}{Exception}"
        }
      },
      {
        "Name": "File",
        "Args": {
          "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss.fff}[{Level:u3}]<{ThreadId,2}>[{SourceContext,33}]-> {Message}{NewLine}{Exception}",
          "path": "C:/Users/Alin Mihailescu/source/repos/WMSOpcClientApp/WMSOpcClient/Logs/log.txt",
          "rollingInterval": "Day"
        }
      }
    ],
    "Enrich": [ "FromLogContext", "WithThreadId" ]
  },
  "RefreshTime": "1000",
